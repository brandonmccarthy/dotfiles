*foreground: #cccccc
*foreground_bold: #a8a19f
*cursor: #cccccc
*background:       #2d2d2d
!!*background: rgba(0, 0, 0, .7)

!! black
*color0: #2d2d2d
*color8: #666666
!! red
*color1: #f2777a
*color9: #ff3334
!! green
*color2: #99cc99
*color10: #9ec400
!! yellow
*color3: #f99157
*color11: #ffcc66
!! blue
*color4: #6699cc
*color12: #6699cc
!! magenta
*color5: #cc99cc
*color13: #ffcc66
!! cyan
